{
  "name": "@gnosis.pm/gnosis-management",
  "version": "0.3.1",
  "description": "GnosisInc Admin Interface",
  "main": "src/index.js",
  "scripts": {
    "build-dev": "NODE_ENV=development ./node_modules/.bin/webpack -p --config webpack.dev.config.js",
    "build-prod": "NODE_ENV=production ./node_modules/.bin/webpack -p --progress --config webpack.prod.config.js",
    "build": "if test \"$NODE_ENV\" = \"production\" ; then npm run build-prod; else npm run build-dev; fi ",
    "clean": "rm -rf dist",
    "start-dev": "node_modules/.bin/webpack-dev-server --hot --host 0.0.0.0 --client-log-level info --config webpack.dev.config.js --progress",
    "start-prod": "NODE_ENV=production node_modules/.bin/webpack-dev-server --host 0.0.0.0 --client-log-level info --config webpack.prod.config.js",
    "start": "if test \"$NODE_ENV\" = \"production\" ; then npm run start-prod; else npm run start-dev; fi ",
    "start:standalone": "cross-env GNOSISDB_HOST='https://gnosisdb-kovan.gnosis.pm' npm start",
    "lint": "eslint ./src --quiet",
    "lint:fix": "eslint ./src --fix --quiet",
    "test": "jest --no-cache",
    "precommit": "./precommit.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosis/gnosis-management.git"
  },
  "author": "Gnosis",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ]
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@gnosis.pm/gnosisjs": "1.1.0",
    "autobind-decorator": "^1.4.1",
    "await-delay": "^1.0.0",
    "babel-polyfill": "^6.23.0",
    "blockies": "0.0.2",
