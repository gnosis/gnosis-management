    "cross-env": "^5.0.5",
    "d3": "^4.9.1",
    "d3-scale-chromatic": "^1.1.1",
    "decimal.js": "^7.2.3",
    "lodash": "4.17.4",
    "moment": "^2.19.2",
    "moment-duration-format": "^1.3.0",
    "normalizr": "^3.2.2",
    "prop-types": "^15.6.0",
