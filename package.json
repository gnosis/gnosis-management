{
  "name": "@gnosis.pm/gnosis-management",
  "version": "0.1.0",
  "description": "GnosisInc Admin Interface",
  "main": "src/index.js",
  "scripts": {
    "build-dev": "NODE_ENV=development ./node_modules/.bin/webpack -p --config webpack.dev.config.js",
    "build-prod": "NODE_ENV=production ./node_modules/.bin/webpack -p --config webpack.prod.config.js",
    "build": "if test \"$NODE_ENV\" = \"production\" ; then npm run build-prod; else npm run build-dev; fi ",
    "clean": "rm -rf dist",
    "start-dev": "node_modules/.bin/webpack-dev-server --hot --host 0.0.0.0 --client-log-level info --config webpack.dev.config.js",
    "start-prod": "NODE_ENV=production node_modules/.bin/webpack-dev-server --host 0.0.0.0 --client-log-level info --config webpack.prod.config.js",
    "start": "if test \"$NODE_ENV\" = \"production\" ; then npm run start-prod; else npm run start-dev; fi ",
    "start:standalone": "cross-env GNOSISDB_HOST='https://gnosisdb-kovan.gnosis.pm' npm start",
    "lint": "eslint ./src --quiet",
    "lint:fix": "eslint ./src --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosis/gnosis-management.git"
  },
  "author": "Gnosis",
  "license": "ISC",
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ]
  },
